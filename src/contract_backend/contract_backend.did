type User = record {
    username: text;
    balance: nat64;
};

type Result = variant {
    Ok;
    Err: text;
};

service : {
    "get_self": () -> (User) query;
    "create_account": (User) -> (opt User);
    "get_balance": () -> (nat64) query;
    "transfer_tokens": (principal, nat64) -> (Result);
}